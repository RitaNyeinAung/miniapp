  padding: 5rpx 10rpx;
  text-align: center;
}
.goods-box .floor-goods .priceimg-box {
  display: flex;
  padding: 0 20rpx 10rpx 20rpx;
}
.goods-box .floor-goods .priceimg-box .price {
  color: #2f1918;
  font-size: 26rpx;
}
.goods-box .floor-goods .priceimg-box .img {
  padding-left: 10rpx;
  width: 80rpx;
  height: 30rpx;
}
