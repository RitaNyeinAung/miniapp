.integralDiv {
  background-color: #f1f1f1;
  height: 100%;
}
.picker-toolbar {
  padding: 20rpx;
  height: 600rpx;
}
.cancel {
  float: left;
}
.confirm {
  float: right;
}
.header {
  padding: 30rpx 0;
  text-align: center;
  border-bottom: 2rpx solid #f1f1f1;
  border-top: 2rpx solid #f1f1f1;
  background-color: #fff;
  margin: 0 15rpx;
}
.header image {
  width: 25rpx;
  height: 20rpx;
  margin: 0 15rpx;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.status {
  padding-top: 20rpx;
  font-size: 30rpx;
  color: #333;
}
.list-wrap {
  background: #fff;
  margin: 13rpx 15rpx;
}
.list-wrap .clearfixli {
  padding: 13rpx;
  height: 241rpx;
  border-bottom: 13rpx solid #f1f1f1;
}
.list-wrap .clearfixli image {
  width: 220rpx;
  height: 200rpx;
}
.list-wrap .clearfixli .list-text {
  width: 430rpx;
  padding-left: 25rpx;
}
.list-wrap .clearfixli .list-text .text {
  font-size: 32rpx;
  color: #333;
  line-height: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.list-wrap .clearfixli .list-text .new-price {
  font-size: 32rpx;
  color: #f23030;
  padding-top: 10rpx;
}
.list-wrap .clearfixli .list-text .new-price .span-inte {
  font-size: 40rpx;
  font-weight: bold;
}
.list-wrap .clearfixli .list-text .new-price .span-inte .em {
  font-style: normal;
  font-size: 25rpx;
  color: #7c7c7d;
  line-height: 25rpx;
  padding: 0 0 0 10rpx;
}
.list-wrap .clearfixli .list-text .status {
  color: #ddd;
  padding-top: 10rpx;
}
.list-wrap .clearfixli .list-text .status .span-st {
  font-size: 24rpx;
}
.list-wrap .clearfixli .list-text .status .span-st:nth-child(2) {
  padding-left: 30rpx;
}
.list-wrap .clearfixli .list-text .show-shop {
  display: flex;
  margin: 15rpx 0 5rpx 0;
}
.list-wrap .clearfixli .list-text .show-shop .show-name {
  font-size: 24rpx;
  padding: 3rpx 10rpx 0 0;
  color: #999999;
}
.list-wrap .clearfixli .list-text .show-shop .btn-shop {
  width: 18rpx;
  height: 20rpx;
  margin: 10rpx 20rpx 0 0;
}
.list-wrap .clearfixli .list-text .show-shop .shop-gradename {
  font-size: 22rpx;
  background: #de2d35;
  color: white;
  border-radius: 5rpx;
  padding: 5rpx 10rpx;
  text-align: center;
}

