.logoin-main {
  padding: 0 60rpx;
  background: #fff;
  border-top: 1rpx solid #f1f1f1;
}
.logoin-main .from .input-main {
  height: 110rpx;
  margin-top: 30rpx;
  position: relative;
  background: #f5f5f5;
  display: flex;
  line-height: 110rpx;
  text-align: center;
}
.logoin-main .from .input-main .icon {
  width: 110rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.logoin-main .from .input-main .poinput {
  width: 100%;
  border: none;
  background: #f5f5f5;
  margin: 20rpx 0 0 110rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 100%;
}
.logoin-main .from .passWord .icon {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 44rpx 51rpx;
}
.logoin-main .from .phone-number .icon {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 36rpx 50rpx;
}
.logoin-main .from .message .icon {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 41rpx 33rpx;
}
.logoin-main .from .message .btn-ver {
  width: 188rpx;
  height: 104rpx;
  position: absolute;
  right: 4rpx;
  top: 50%;
  margin-top: -52rpx;
  border: none;
  background: #fff;
  font-size: 26rpx;
  color: #333;
  border-radius: 10rpx;
  outline: none;
}
.logoin-main .from .message .btn-ver:active {
  background: #ccc;
  color: #333;
}
.logoin-main .from .message .btn-ver.active {
  background: #ccc;
  color: #333;
}
.logoin-main .btn-in {
  width: 100%;
  height: 110rpx;
  border: none;
  border-radius: 40rpx;
  outline: none;
  margin-top: 30rpx;
  background: #d02629;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 110rpx;
}

