.shopHome .btm .parentDiv .cssalert {
  z-index: 999;
  position: absolute;
  background-color: #fff;
  left: 20rpx;
  width: 180rpx;
  padding: 0 20rpx;
  bottom: 90rpx;
  height: 300rpx;
  overflow: auto;
  margin-bottom: -68rpx;
  border-radius: 10rpx;
  border: 1rpx solid #e6e6e8;
}
.shopHome .btm .parentDiv .cssalert ._p {
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-bottom: 1rpx solid #e6e6e8;
}
.shopHome .btm .parentDiv .cssalert ._p:last-child {
  border: 0;
}
.shopHome .btm .parentDiv .cssalert .rotate {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  margin-top: 100rpx;
  border: 2rpx solid gray !important;
  border-bottom-color: transparent !important;
}
.shopHome .btm .parentDiv .cssalert .rotate {
  display: inline-block;
  -webkit-animation: rotate 0.6s linear infinite;
  animation: rotate 0.6s linear infinite;
}
@-webkit-keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
}
}
@keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}

