.wrap {
  padding: 0 60rpx;
  background: #fff;
  border-top: 1rpx solid #f1f1f1;
}
.wrap .input-main {
  height: 110rpx;
  margin-top: 30rpx;
  position: relative;
  display: flex;
  background: #f5f5f5;
}
.wrap .input-main .icon {
  width: 110rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.wrap .input-main .rinput {
  width: 100%;
  border: none;
  background: #f5f5f5;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 100%;
  margin: 20rpx 0 0 110rpx;
}
.wrap .passWord .icon {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 44rpx 51rpx;
}
.wrap .btn-in {
  width: 100%;
  height: 110rpx;
  border: none;
  border-radius: 110rpx;
  outline: none;
  margin-top: 30rpx;
  background: #d02629;
  font-size: 36rpx;
  color: #fff;
  margin-bottom: 50rpx;
  text-align: center;
  line-height: 110rpx;
}
.wrap .btn-in:active {
  box-shadow: 0 10rpx 10rpx #ccc;
}

