.nav-wrap {
  width: 100%;
  overflow-x: auto;
  background: #fff;
}
.nav-wrap .nav {
  width: 1239rpx;
  height: 94rpx;
  border-bottom: 2rpx solid #dfdfdd;
}
.nav-wrap .nav .li {
  width: 177rpx;
  height: 100%;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  float: left;
}
.nav-wrap .nav .li .span {
  display: block;
  color: #676767;
  font-size: 30rpx;
  line-height: 54rpx;
  height: 54rpx;
  margin: 20rpx 0;
  border-right: 2rpx solid #dfdfdd;
}
.nav-wrap .nav .li:last-child .span {
  border-right: 0;
}
.nav-wrap .nav .active {
  border-bottom: 6rpx solid #fd4f4b;
}
.nav-wrap .nav .active .span {
  color: #ff781a;
}
.order-list {
  background: #fff;
  line-height: 70rpx;
  margin-bottom: 20rpx;
}
.order-list .hd {
  margin: 0 20rpx;
  border-bottom: 2rpx solid #dfdfdd;
  height: 70rpx;
}
.order-list .hd .title {
  font-size: 29rpx;
  color: #333;
}
.order-list .hd .status {
  font-size: 30rpx;
  color: #d02629;
}
.order-list .hd .status.light {
  color: #adadad;
}
.order-list .hd .status.ash {
  color: #333;
}
.order-list .describe {
  margin: 0 20rpx;
  padding: 13rpx 0;
  border-bottom: 2rpx solid #dfdfdd;
  position: relative;
}
.order-list .describe .btn {
  position: absolute;
  right: 0;
  bottom: 10rpx;
  width: 150rpx;
  height: 50rpx;
  border: 2rpx solid #ff881e;
  background: #fff;
  border-radius: 40rpx;
  color: #ff881e;
}
.order-list .describe image {
  width: 146rpx;
  height: 146rpx;
}
.order-list .describe .words {
  width: 524rpx;
}
.order-list .describe .words .number {
  font-size: 22rpx;
  color: #fd5149;
}
.order-list .describe .words .price {
  font-size: 28rpx;
  color: #333;
  line-height: 62rpx;
}
.order-list .describe .words .add {
  font-size: 25rpx;
  margin: 63rpx 0 0 0;
  line-height: 30rpx;
}
.order-list .describe .words .text {
  padding: 0 20rpx;
  font-size: 32rpx;
  color: #333;
  line-height: 45rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.order-list .commod {
  padding: 0 20rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #dfdfdd;
  position: relative;
}
.order-list .commod .total {
  font-size: 30rpx;
  color: #333;
}
.order-list .commod .actual {
  padding-left: 70rpx;
  font-size: 30rpx;
  color: #CD0C11;
}
.order-list .commod .icon {
  font-size: 30rpx;
  color: #333;
  position: absolute;
  right: 35rpx;
  top: 50%;
  width: 38rpx;
  height: 47rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-top: -23.5rpx;
}
.order-list .operation {
  height: 90rpx;
  padding: 10rpx 20rpx;
  line-height: 70rpx;
}
.order-list .operation .timer {
  font-size: 25rpx;
  color: #757575;
}
.order-list .operation .butn {
  width: 150rpx;
  height: 70rpx;
  margin-right: 10rpx;
  background: #fff;
  border: 2rpx solid #d02629;
  font-size: 27rpx;
  color: #d02629;
  border-radius: 10rpx;
  outline: none;
}
.order-list .operation .btn {
  width: 150rpx;
  height: 70rpx;
  background: #fff;
  border: 2rpx solid #d02629;
  font-size: 27rpx;
  color: #d02629;
  border-radius: 10rpx;
  outline: none;
  line-height: 70rpx;
  text-align: center;
}
.order-list .operation .repeat {
  width: 130rpx;
  height: 70rpx;
  background: #fff;
  border: 2rpx solid #d02629;
  font-size: 27rpx;
  color: #d02629;
  margin: 0 10rpx;
  border-radius: 10rpx;
  outline: none;
}
.whole-wrap {
  background: #f1f1f1;
}
.order-wrap {
  padding-top: 10rpx;
  background-color: #f1f1f1;
}
.order-wrap .comm-null {
  padding-top: 50rpx;
}
.order-wrap .comm-null .p {
  font-size: 28rpx;
  color: #666;
  padding-top: 20rpx;
}
.up-warp {
  height: 50rpx;
  padding: 30rpx 0;
  text-align: center;
}
.up-warp .rotate {
  display: inline-block;
  vertical-align: middle;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 2rpx solid gray;
  margin-right: 12rpx;
  border-bottom-color: transparent;
}
.up-warp .rotate {
  -webkit-animation: rotate .6s linear infinite;
  animation: rotate .6s linear infinite;
}
@-webkit-keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
}
}
@keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.up-warp .load-title {
  font-size: 24rpx;
  color: gray;
  display: inline-block;
  vertical-align: middle;
}
.no-data {
  background-color: #fff;
  height: 50rpx;
  padding: 30rpx 0;
  text-align: center;
  font-size: 24rpx;
  color: gray;
}

