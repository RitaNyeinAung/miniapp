.business {
  background-color: #f1f1f1;
  position: absolute;
}
.hd {
  padding: 20rpx;
}
.hd .title {
  border-left: 6rpx solid #d02629;
  padding-left: 20rpx;
  font-size: 32rpx;
  color: #111111;
}
.per-center ._li {
  background-color: #fff;
  border-bottom: 2rpx solid #F1F1F1;
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 0 20rpx;
}
.per-center ._li ._span {
  width: 200rpx;
  font-size: 32rpx;
  color: #BDBDBD;
}
.per-center ._li .mint-datetime-action {
  color: #ff8000;
}
.per-center ._li u-input {
  width: 500rpx;
  border: 0;
  font-size: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.per-center .mint-datetime-action {
  color: #ff8000;
}
.per-center .little-hint {
  margin: 0;
}
.per-center .little-hint ._i {
  font-style: normal;
  color: red;
}
.per-center .listHead {
  margin-bottom: 10rpx;
  position: relative;
}
.per-center .listHead .delete {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  display: block;
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 50rpx;
  top: 0;
}
.per-center .listHead .select {
  padding: 20rpx;
}
.per-center .listHead .select .address-area-tit {
  padding: 0;
  box-shadow: none;
  border-bottom: 0;
  line-height: 82rpx;
  height: 82rpx;
}
.per-center .listHead .select .address-area-tit ._li {
  padding: 0 20rpx;
  height: 80rpx;
  width: 33%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  font-size: 24rpx;
  display: inline-block;
  border-bottom: 2rpx solid #ccc;
}
.per-center .listHead .select .address-area-tit ._li.active {
  border-bottom: 2rpx solid #ff8000;
  color: #ff8000;
}
.per-center .addList {
  height: 150rpx;
  background-color: #F1F1F1;
}
.per-center ._a {
  width: 10000% !important;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.per-center ._a u-input {
  width: 500rpx;
  height: 90rpx;
  border: 0;
  font-size: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.per-center .content {
  display: block;
  width: 5rpx;
}
.per-center .rightImg {
  width: 24rpx;
  height: 44rpx;
}
.per-center .time-length {
  height: 470rpx;
  overflow-y: scroll;
}
.per-center .goods-list {
  margin-top: 20rpx;
  height: 450rpx;
  overflow-y: scroll;
}
.per-center .goods-list ._li {
  height: 60rpx;
  padding: 0 70rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}
.per-center .goods-list ._li.selected-li {
  color: #ff8000;
}
.tol {
  width: 100%;
}
.add-category {
  margin: 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
  border: 2rpx solid #c2c2c2;
}
.button {
  margin: 75rpx 20rpx 0;
  height: 90rpx;
  color: #fff;
  background-color: #d02629;
  border-radius: 90rpx;
  border: 0;
  width: 710rpx;
  outline: none;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.p {
  padding: 20rpx;
  margin: 30rpx 0 40rpx;
  font-size: 24rpx;
  color: #959595;
  text-align: left;
}
.picker-toolbar {
  height: 80rpx;
}
.mint-popup-4 {
  width: 100%;
}
.u-drawer-left {
  width: 100% !important;
  padding: 20rpx;
  height: 700rpx;
  box-sizing: border-box;
  bottom: 0;
  margin-top: 560rpx;
}
.mint-datetime-action {
  color: #ff8000;
  width: 50%;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
.mint-datetime-confirm {
  float: right;
}
.mint-datetime-cancel {
  float: left;
}

