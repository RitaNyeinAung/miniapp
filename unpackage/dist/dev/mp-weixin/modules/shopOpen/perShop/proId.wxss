.prold {
  background: #f1f1f1;
  position: absolute;
}
.tol {
  width: 100%;
}
.picker-toolbar {
  height: 120rpx;
}
.u-drawer-left {
  width: 100% !important;
  padding: 20rpx;
  height: 700rpx;
  box-sizing: border-box;
  bottom: 0;
  margin-top: 700rpx;
  font-size: 32rpx;
  color: #bdbdbd;
}
.mint-datetime-action {
  width: 50%;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
.mint-datetime-confirm {
  float: right;
}
.mint-datetime-cancel {
  float: left;
}
.like {
  background-color: #fff;
  border-bottom: 1rpx solid #f1f1f1;
  height: 90rpx;
  font-size: 28rpx;
  padding: 20rpx;
}
.idPhotos {
  border-bottom: 1rpx solid #fff;
  padding: 0 27rpx;
  background-color: #fff;
}
.idPhotos ._h4 {
  padding: 30rpx 0 25rpx;
  color: #BDBDBD;
  font-size: 32rpx;
}
.idPhotos .first-upload {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  font-size: 32rpx;
  color: #959595;
}
.idPhotos .first-upload .sec-upload {
  width: 300rpx;
  height: 300rpx;
  border: 1rpx dashed  #BDBDBD;
}
.idPhotos .first-upload .sec-upload .images {
  width: 100%;
  height: 100%;
}
.idPhotos .first-upload .input-btn {
  width: 300rpx;
  height: 300rpx;
}
.idPhotos .first-upload .input-btn .frame {
  width: 100%;
  height: 100%;
}
.idPhotos .p {
  margin: 30rpx 0 40rpx;
  font-size: 24rpx;
  color: #959595;
  text-align: left;
}
.per-center .li {
  background-color: #fff;
  border-bottom: 1px solid #f1f1f1;
  height: 90rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 0 20rpx;
}
.per-center .li .span {
  width: 220rpx;
  font-size: 32rpx;
  color: #bdbdbd;
}
.per-center .li input {
  width: 520rpx;
  height: 90rpx;
  border: 0;
  font-size: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.per-center .li .div2 {
  font-size: 32rpx;
  display: flex;
  align-items: center;
  color: #bdbdbd;
}
.per-center .view1 {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.per-center .rightImg {
  margin-left: 20rpx;
  width: 24rpx;
  height: 44rpx;
}
button {
  margin: 45rpx 20rpx 25rpx;
  height: 90rpx;
  color: #fff;
  background-color: #d02629;
  border-radius: 90rpx;
  border: 0;
  width: 710rpx;
  outline: none;
  font-size: 32rpx;
}
.p {
  text-align: center;
  margin-bottom: 45rpx;
}
.p .reds {
  color: #fe0100;
}

