.navbar {
  margin-top: 5px;
  display: flex;
  width: 100%;
  height: 80rpx;
  background: #fff;
  z-index: 10;
}
.navbar .nav-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  font-size: 30rpx;
  color: #303133;
  position: relative;
}
.navbar .nav-item.current {
  color: #c3996b;
}
.navbar .nav-item.current:after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 120rpx;
  height: 0;
  border-bottom: 4rpx solid #c3996b;
}
.navbar .p-box {
  display: flex;
  flex-direction: column;
}
.navbar .p-box .yticon {
  display: flex;
