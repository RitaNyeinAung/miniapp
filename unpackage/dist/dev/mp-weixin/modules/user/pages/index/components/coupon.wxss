.couponDiv {
  background-color: #f1f1f1;
  height: 100%;
  width: 100%;
}
.mainDiv {
  background-color: #f1f1f1;
}
.text-center {
  text-align: center;
}
.fl {
  float: left;
}
.text-right {
  text-align: right;
}
.fr {
  float: right;
}
.data-status {
  line-height: 100rpx;
  font-size: 40rpx;
  color: #999;
}
.Navigation {
  height: 90rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 0;
  border-bottom: 2rpx solid #f5f5f5;
  border-top: 2rpx solid #f5f5f5;
  background: #fff;
  display: flex;
  text-align: center;
}
.Navigation .li {
  width: 33.33333%;
  border-right: 2rpx solid #ccc;
  box-sizing: border-box;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #666;
}
.Navigation .li:last-child {
  border: none;
}
.Navigation .li.active {
  color: #d02629;
}
.content-wrap {
  padding: 32rpx 20rpx;
}
.content-wrap .conli {
  width: 100%;
  height: 220rpx;
  padding-left: 27rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.content-wrap .conli .item-pull-left {
  width: 400rpx;
}
.content-wrap .conli .item-pull-right {
  width: 283rpx;
  padding-right: 25rpx;
  box-sizing: border-box;
  padding-top: 42rpx;
}
.content-wrap .conli .item-pull-right .price {
  color: #fff;
  font-size: 24rpx;
}
.content-wrap .conli .item-pull-right .price .pm {
  font-size: 50rpx;
  font-family: 'Aparajita';
}
.content-wrap .conli .button {
  width: 126rpx;
  height: 55rpx;
  border-radius: 10rpx;
  border: none;
  background: none;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 55rpx;
  float: right;
}
.content-wrap .conli .status {
  font-size: 24rpx;
  color: #E02828;
  padding-bottom: 10rpx;
}
.content-wrap .conli .h2 {
  font-size: 36rpx;
  padding-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content-wrap .conli .term {
  font-size: 22rpx;
  color: #a5a5a5;
  padding-top: 10rpx;
}
.content-wrap .conli .timer {
  padding-top: 10rpx;
  font-size: 25rpx;
  color: #757575;
}
.content-wrap .conli.origin {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.content-wrap .conli.origin .h2 {
  color: #E18752;
}
.content-wrap .conli.origin .button {
  background: #e02828;
  box-shadow: 8rpx 8rpx 0 #da8a12;
}
.content-wrap .conli.green {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.content-wrap .conli.green .h2 {
  color: #86B766;
}
.content-wrap .conli.green .button {
  background: #438524;
  box-shadow: 8rpx 8rpx 0 #68A24B;
}
.content-wrap .conli.yellow {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.content-wrap .conli.yellow .h2 {
  color: #D3B881;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content-wrap .conli.yellow .status {
  font-size: 24rpx;
  color: #E02828;
  padding-bottom: 10rpx;
}
.content-wrap .conli.yellow .button {
  background: #E02828;
  box-shadow: 8rpx 8rpx 0 #da8a12;
}
.content-wrap .conli.gray {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.content-wrap .conli.gray .h2 {
  color: #666;
}
.content-wrap .conli.gray .button {
  background: #aaa9a9;
  box-shadow: 8rpx 8rpx 0 #b8b8b8;
}

