<view class="shopLists"><view class="lit-ul"><view data-event-opts="{{[['tap',[['goToShopHome',['$event']]]]]}}" class="header" bindtap="__e"><text class="lit-txt">全部宝贝</text><image src="/static/images/common/btn-right.png"></image></view><u-collapse class="shoplevel-coll" vue-id="1fd2b4e6-1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{shopLevel}}" wx:for-item="outers" wx:for-index="index" wx:key="id"><block wx:if="{{outers.level==0}}"><u-collapse-item class="shoplevel-item" vue-id="{{('1fd2b4e6-2-'+index)+','+('1fd2b4e6-1')}}" bind:__l="__l" vue-slots="{{['default','title']}}"><view class="first" slot="title"><text class="shoplevel1-name">{{outers.class_name}}</text><image class="shoplevel1-img" src="/static/images/common/btn-right.png" data-event-opts="{{[['tap',[['goToLevel',['$0','$1'],[[['shopLevel','id',outers.id,'id']],[['shopLevel','id',outers.id,'class_name']]]]]]]}}" bindtap="__e"></image></view><block wx:for="{{shopLevel}}" wx:for-item="cores" wx:for-index="__i0__" wx:key="id"><block wx:if="{{cores.level==1&&cores.f_id==outers.id}}"><view data-event-opts="{{[['tap',[['goToLevelTwo',['$0','$1'],[[['shopLevel','id',cores.id,'id']],[['shopLevel','id',cores.id,'class_name']]]]]]]}}" class="second" bindtap="__e"><text class="shoplevel2-name">{{cores.class_name}}</text><image class="shoplevel2-img" src="/static/images/common/btn-right.png"></image></view></block></block><block wx:for="{{shopLevel}}" wx:for-item="core" wx:for-index="__i1__" wx:key="id"><block wx:if="{{core.level==2&&core.f_id==cores.id}}"><view data-event-opts="{{[['tap',[['goToLevelThree',['$0','$1'],[[['shopLevel','id',core.id,'id']],[['shopLevel','id',core.id,'class_name']]]]]]]}}" class="third" bindtap="__e"><text class="shoplevel3-name">{{core.class_name}}</text><image class="shoplevel3-img" src="/static/images/common/btn-right.png"></image></view></block></block></u-collapse-item></block></block></u-collapse></view></view>